doctype
html.no-js(lang='en', prefix='og: http://ogp.me/ns#')

    include includes/_head

    body

        include includes/_header

        // Main
        main

            .container.container--column
                h2.h2-title.margin-top Key Dates

            .container.container--hor-center
                .keydates
                    ul.keydates__list
                        li.keydates__item
                            time(datetime='2016-02-01').keydates__date Feb 1 2016
                            span.keydates__event Registration and abstracts submission deadline
                        li.keydates__item
                            time(datetime='2016-03-01').keydates__date Mar 1 2016
                            span.keydates__event Notification of acceptance/rejection
                        li.keydates__item
                            time(datetime='2016-04-29').keydates__date Apr 29 2016
                            span.keydates__event Registration and fee payment deadline
                        li.keydates__item
                            time(datetime='2016-06-01').keydates__date Jun 1 2016
                            span.keydates__event Full papers submission deadline
                        li.keydates__item
                            time(datetime='2016-07-01').keydates__date Jul 1-3 2016
                            span.keydates__event Conference dates


        include includes/_footer

